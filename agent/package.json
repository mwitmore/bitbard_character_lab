        "@ai-sdk/amazon-bedrock": "1.1.6",
        "@ai-sdk/anthropic": "1.1.6",
        "@ai-sdk/google": "1.1.0",
        "@ai-sdk/groq": "1.1.7",
        "@ai-sdk/mistral": "1.1.15",
        "@ai-sdk/openai": "1.1.9",
        "@jest/globals": "^29.7.0",
        "@types/jest": "29.5.14",
        "ai": "4.1.54",
        "glob": "11.0.1",
        "jest": "29.7.0",
        "js-tiktoken": "1.0.19",
